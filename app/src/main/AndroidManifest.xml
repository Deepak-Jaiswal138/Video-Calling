<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deepak.videocall">

    <application
        android:allowBackup="true"
        android:icon="@drawable/video_call"
        android:label="@string/app_name"
        android:roundIcon="@drawable/video_call"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VideoChatActivity"></activity>
        <activity android:name=".CallingActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".FindPeopleActivity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".SettingsActivity" />
        <activity
            android:name=".ContactActivity"
            android:label="@string/title_activity_main" />
        <activity android:name=".RegistrationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>